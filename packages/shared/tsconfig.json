{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "noEmit": false,
    "paths": {
      "@magicborn/shared": ["src"],
      "@magicborn/shared/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "../../types/**/*.d.ts"],
  "exclude": ["node_modules", "dist"]
}



