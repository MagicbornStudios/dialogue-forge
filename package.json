{
  "name": "@magicborn/dialogue-forge",
  "version": "0.1.8",
  "description": "Visual node-based dialogue editor with Yarn Spinner support",
  "author": "Ben Garrard <b2gdevs@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MagicbornStudios/dialogue-forge.git"
  },
  "bugs": {
    "url": "https://github.com/MagicbornStudios/dialogue-forge/issues"
  },
  "homepage": "https://github.com/MagicbornStudios/dialogue-forge#readme",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "dialogue-forge": "bin/dialogue-forge.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist",
    "dist/styles",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "next build",
    "build:types": "payload generate:types",
    "build:lib": "tsc -p tsconfig.lib.json && tsc -p tsconfig.lib.json --module esnext --outDir dist/esm && node ./bin/postbuild.js",
    "dev": "next dev --turbo",
    "dev:watch": "tsc -p tsconfig.lib.json --watch",
    "start": "next start",
    "payload:generate": "payload generate:types",
    "payload:migrate": "payload migrate",
    "payload:seed": "tsx app/payload-seed-standalone.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build:lib",
    "postinstall": "node scripts/patch-undici.js",
    "sync": "./sync-to-repo.sh",
    "pack": "npm pack --dry-run",
    "vendor:opencode:install": "cd vendor/opencode && bun install",
    "vendor:opencode:dev": "cd vendor/opencode/packages/console/app && bun run dev",
    "vendor:opencode:build": "cd vendor/opencode/packages/console/app && bun run build",
    "vendor:opencode:sync": "node scripts/sync-opencode-ui.js",
    "vendor:opencode:update": "node scripts/update-opencode-submodule.js",
    "vendor:opencode:update-and-build": "npm run vendor:opencode:update && npm run vendor:opencode:build && npm run vendor:opencode:sync"
  },
  "keywords": [
    "dialogue",
    "editor",
    "yarn-spinner",
    "node-graph",
    "interactive-fiction"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "overrides": {
    "undici": "^6.19.8"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.10",
    "@copilotkit/react-core": "^1.51.2",
    "@copilotkit/react-ui": "^1.51.2",
    "@copilotkit/runtime": "^1.51.2",
    "@dagrejs/dagre": "^1.1.8",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@lexical/code": "^0.35.0",
    "@lexical/link": "^0.35.0",
    "@lexical/list": "^0.35.0",
    "@lexical/markdown": "^0.35.0",
    "@lexical/react": "^0.35.0",
    "@lexical/rich-text": "^0.35.0",
    "@lexical/table": "^0.35.0",
    "@payloadcms/db-sqlite": "^3.70.0",
    "@payloadcms/next": "^3.70.0",
    "@payloadcms/richtext-lexical": "^3.70.0",
    "@payloadcms/sdk": "^3.70.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.16",
    "@types/d3-hierarchy": "^3.1.7",
    "ai": "^6.0.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3-hierarchy": "^3.1.2",
    "graphql": "^16.12.0",
    "immer": "^11.1.3",
    "lexical": "^0.35.0",
    "lucide-react": "^0.559.0",
    "openai": "^5.23.2",
    "payload": "^3.70.0",
    "react-hotkeys-hook": "^5.2.3",
    "react-resizable-panels": "^4.4.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.30.0",
    "reactflow": "^11.11.4",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0",
    "vite-tsconfig-paths": "^6.0.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.33.0",
    "next": "^15.5.9",
    "patch-package": "^8.0.1",
    "postcss": "^8.4.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
